{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/ivi-analog-back/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/ivi-analog-back/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/ivi-analog-back",
  "projects": {
    "ivi-analog-back": {
      "type": "application",
      "root": "apps/ivi-analog-back",
      "entryFile": "main",
      "sourceRoot": "apps/ivi-analog-back/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ivi-analog-back/tsconfig.app.json"
      }
    },
    "parser": {
      "type": "application",
      "root": "apps/parser",
      "entryFile": "main",
      "sourceRoot": "apps/parser/src",
      "compilerOptions": {
        "tsConfigPath": "apps/parser/tsconfig.app.json"
      }
    },
    "rabbit": {
      "type": "library",
      "root": "libs/rabbit",
      "entryFile": "index",
      "sourceRoot": "libs/rabbit/src",
      "compilerOptions": {
        "tsConfigPath": "libs/rabbit/tsconfig.lib.json"
      }
    },
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "film": {
      "type": "application",
      "root": "apps/film",
      "entryFile": "main",
      "sourceRoot": "apps/film/src",
      "compilerOptions": {
        "tsConfigPath": "apps/film/tsconfig.app.json"
      }
    }
  }
}